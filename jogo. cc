	beq $9, $0, pil_esquerda_cima2             
	sw $4,3604($8)
	sw $4,4116($8)
	sw $4,4628($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j contpil_esquerda_cima1

pil_esquerda_cima2:
	lui $8, 0x1001
	li $9,2
contpil_esquerda_cima2:                       
	beq $9, $0, pil_direita_cima1              
	sw $6,3604($8)
	sw $6,4628($8)
	addi $8, $8, 8
	addi $9, $9, -1
	j contpil_esquerda_cima2
	
pil_direita_cima1:
	lui $8, 0x1001
	li $9,3
contpil_direita_cima1:                       
	beq $9, $0, pil_direita_cima2             
	sw $4,3552($8)
	sw $4,4064($8)
	sw $4,4576($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j contpil_direita_cima1

pil_direita_cima2:
	lui $8, 0x1001
	li $9,2
contpil_direita_cima2:                       
	beq $9, $0, pil_direita_baixo1              
	sw $6,3552($8)
	sw $6,4576($8)
	addi $8, $8, 8
	addi $9, $9, -1
	j contpil_direita_cima2

pil_direita_baixo1:
	lui $8, 0x1001
	li $9,3
contpil_direita_baixo1:                       
	beq $9, $0, pil_direita_baixo2             
	sw $4,28128($8)
	sw $4,28640($8)
	sw $4,29152($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j contpil_direita_baixo1

pil_direita_baixo2:
	lui $8, 0x1001
	li $9,2
contpil_direita_baixo2:                       
	beq $9, $0, copy_map_to_background              
	sw $6,28128($8)
	sw $6,29152($8)
	addi $8, $8, 8
	addi $9, $9, -1
	j contpil_direita_baixo2
# ... (Seu código main e todo o desenho do mapa, terminando na rotina 'pil_direita_baixo2')
#     Após 'pil_direita_baixo2:', seu programa deve continuar aqui para copiar o mapa.

# --- Seção de Cópia do Mapa para Fundo ---

copy_map_to_background:
    # Registradores que vamos usar:
    # $8:   Vai ser o ponteiro que lê da tela principal (começa em 0x10010000 e avança)
    # $16:  Vai ser o ponteiro que escreve na área da cópia de fundo (começa em 0x10010000 + 32768 e avança)
    # $9:   Contador do loop (número total de UGs a copiar: 8192)
    # $17:  Registrador temporário para carregar/salvar a UG atual

    # 1. Reinicia o ponteiro da tela principal ($8) para o início (0x10010000)
    lui $8, 0x1001      # $8 = 0x10010000

    # 2. Configura o ponteiro da tela de fundo ($16)
    # Ele começa no endereço base da tela principal ($8) + o tamanho da tela (32768 bytes)
    addi $16, $8, 32768 # $16 = 0x10010000 + 32768 (O SEU CALCULO! 0x10018000 em hexa)

    # 3. Configura o contador do loop ($9)
    # Temos 8192 UGs no total (512/4 largura * 256/4 altura = 128 * 64 = 8192 UGs)
    li $9, 8192         # $9 = 8192

copy_loop:
    beq $9, $0, end_copy_map_to_background # Se o contador $9 chegar a zero, a cópia está completa

    lw $17, 0($8)       # Carrega 1 UG (4 bytes) da tela principal (no endereço apontado por $8) para $17
    sw $17, 0($16)      # Salva essa UG (que está em $17) na área da cópia de fundo (no endereço apontado por $16)

    addi $8, $8, 4      # Avança o ponteiro da tela principal em 4 bytes (para a próxima UG)
    addi $16, $16, 4    # Avança o ponteiro da cópia de fundo em 4 bytes (para a próxima UG)
    addi $9, $9, -1     # Decrementa o contador de UGs copiadas

    j copy_loop         # Volta para o início do loop

end_copy_map_to_background:
    # A partir deste ponto, a sua tela de fundo (mapa sem fantasmas) está
    # completamente copiada e salva a partir do endereço 0x10010000 + 32768.

    # Agora você pode desenhar seus fantasmas e Pac-Man em suas posições iniciais
    # na tela principal (que ainda está em 0x10010000)
    # e iniciar a lógica de movimento.

    # Exemplo (você continuaria com o desenho dos fantasmas e o loop de jogo aqui)
    # fantv1: ...
    # fantr1: ...
    # fantc1: ...
    # pacman1: ...
    
    #pacman
pacman1:
	lui $8, 0x1001
	li $9, 3
cont1pacman:
	beq $9, $0, pacman2
	sw $6, 2100($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j cont1pacman
	
pacman2:
	lui $8, 0x1001
	li $9, 2
cont2pacman:
	beq $9, $0, pacman3
	sw $6, 2608($8)
	addi $8, $8, 16
	addi $9, $9, -1
	j cont2pacman
	
pacman3:
	lui $8, 0x1001
	li $9, 3
cont3pacman:
	beq $9, $0, pacman4
	sw $6, 3116($8)
	addi $8, $8, 512
	addi $9, $9, -1
	j cont3pacman
	
pacman4:
	lui $8, 0x1001
	li $9, 2
cont4pacman:
	beq $9, $0, pacman5
	sw $6, 4656($8)
	addi $8, $8, 16
	addi $9, $9, -1
	j cont4pacman
	
pacman5:
	lui $8, 0x1001
	li $9, 3
cont5pacman:
	beq $9, $0, pacman6
	sw $6, 5172($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j cont5pacman
	
pacman6:
	lui $8, 0x1001
	li $9, 2
cont6pacman:
	beq $9, $0, pacman7
	sw $6, 3140($8)
	addi $8, $8, 1024
	addi $9, $9, -1
	j cont6pacman
	
pacman7:
	lui $8, 0x1001
	li $9, 1
cont7pacman:
	beq $9, $0, pacman8
	sw $6, 3644($8)
	addi $9, $9, -1
	j cont7pacman
	
pacman8:
	lui $8, 0x1001
	li $9, 3
cont8pacman:
	beq $9, $0, pacman9
	sw $4, 2612($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j cont8pacman

pacman9:
	lui $8, 0x1001
	li $9, 5
cont9pacman:
	beq $9, $0, pacman10
	sw $4, 3120($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j cont9pacman
	
pacman10:
	lui $8, 0x1001
	li $9, 3
cont10pacman:
	beq $9, $0, pacman11
	sw $4, 3632($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j cont10pacman
	
pacman11:
	lui $8, 0x1001
	li $9, 5
cont11pacman:
	beq $9, $0, pacman12
	sw $4, 4144($8)
	addi $8, $8, 4
	addi $9, $9, -1
	j cont11pacman
	
pacman12:
	lui $8, 0x1001
	li $9, 3
cont12pacman:                       
	beq $9, $0, inicializar              
	sw $4, 4660($8)                
	addi $8, $8, 4
	addi $9, $9, -1
	j cont12pacman
#>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
# Carrega o endereço base da tela principal em $22
inicializar: 
	lui $18, 0x1001      # $22 = 0x10010000 (Base da tela principal)
# Carrega o endereço base da cópia do background em $23
        addi $23, $18, 32768 # $23 = 0x10010000 + 32768 = 0x10018000 (Base do background)
#inicializar fantasma vermelho
	lui $20, 0x1001
	addi $20, $20, 10324
	move $28, $20
	addi $26, $20, 96
	addi $11, $26, 8192
	addi $29, $28, 8192
	li $21, 4
	move $4, $20
	jal draw_red_ghost
#inicializar fantasma verde	
	lui $25, 0x1001
	addi $25, $25, 2292
	move $30, $25
	addi $14, $25, 24576
	li $27, 512
	
	move $4, $25
	jal desenharFantasmaVerde

#inicializar fantasma laranja
	lui $12, 0x1001
	addi $12, $12, 27520
	move $13, $12
	addi $22, $12, -25600
	li $15, -512
	move $4, $12
	
	jal desenhar_laranja
	
	
	j game_loop

game_loop:
	jal delay
	move $4, $20         # Passa a posição ATUAL do fantasma ($20) para $4 (a0)
    	move $5, $18        # Passa a base da tela principal ($16) para $5 (a1)
    	move $6, $23         # Passa a base da cópia de fundo ($17) para $6 (a2)
   	jal erase_red_ghost  # Chama a função que apaga o fantasma da tela
	add $20, $20, $21
	
	beq $20,$26, desce
	beq $20, $28, inverte
	beq $20, $11, esquerda
	beq $20, $29, sobe
	
	
	
	
	j desenha_vermelho
sobe:
	li $21, -512
	j desenha_vermelho
esquerda:
	li $21, -4	
	j desenha_vermelho
desce:
	li $21, 512
	j desenha_vermelho	
inverte:
	li $21, 4
	
desenha_vermelho:
	move $4, $20
	jal draw_red_ghost
	
	
	move $4, $25         # Passa a posição ATUAL do fantasma ($20) para $4 (a0)
    	move $5, $18        # Passa a base da tela principal ($16) para $5 (a1)
    	move $6, $23
    	
    	jal erase_green_ghost
    	
    	add $25, $25, $27
    	
	beq $25, $30, inverteVerde
    	beq $25, $14, inverteVerde
    	j d_Verde
	
inverteVerde:
	sub $27, $0, $27	
d_Verde:	
	move $4, $25
    	jal desenharFantasmaVerde
    	
    	move $4, $12
    	move $5, $18
    	move $6, $23
    	
    	jal erase_laranja
    	
    	add $12, $12, $15
    	beq $12, $22, inverte_laranja
    	beq $12, $13, inverte_laranja
    	
    	move $4, $12
    	jal desenhar_laranja
    	
    	
 
    	j game_loop
inverte_laranja:
	sub $15, $0, $15
	j game_loop

	
draw_red_ghost:
    # Cabeça e corpo do fantasma (usando a cor $3, que é vermelho)
    sw $3, 0($4)   # UG no ponto de referência
    sw $3, 4($4)   # UG 1 à direita
    sw $3, 8($4)   # UG 2 à direita

    sw $3, 512($4)  # UG 1 linha abaixo do ponto de referência
    sw $3, 508($4)  # UG 1 linha abaixo e 1 à esquerda (se 512 é uma linha completa)
    sw $3, 516($4)  # UG 1 linha abaixo e 1 à direita
    sw $3, 520($4)  # UG 1 linha abaixo e 2 à direita
    sw $3, 524($4)  # UG 1 linha abaixo e 3 à direita

    sw $3, 1016($4) # UG 2 linhas abaixo e X à esquerda/direita
    sw $7, 1020($4)
    sw $2, 1024($4)
    sw $7, 1032($4)
    sw $2, 1036($4)
    sw $3, 1028($4) # UG 2 linhas abaixo e Y à esquerda/direita
    sw $3, 1040($4) # UG 2 linhas abaixo e Z à esquerda/direita

    sw $3, 1528($4) # UG 3 linhas abaixo e X à esquerda/direita
    sw $7, 1532($4)
    sw $2, 1536($4)
    sw $3, 1540($4) # UG 3 linhas abaixo e Y à esquerda/direita
    sw $7, 1544($4)
    sw $2, 1548($4)
    sw $3, 1552($4) # UG 3 linhas abaixo e Z à esquerda/direita
    
    sw $3, 2040($4)
    sw $3, 2044($4)
    sw $3, 2048($4)
    sw $3, 2052($4)
    sw $3, 2056($4)
    sw $3, 2060($4)
    sw $3, 2064($4)
    
    sw $3, 2552($4)
    sw $3, 2556($4)
    sw $3, 2560($4)
    sw $3, 2564($4)
    sw $3, 2568($4)
    sw $3, 2572($4)
    sw $3, 2576($4)
    
    sw $3, 3064($4)
    sw $3, 3072($4)
    sw $3, 3080($4)
    sw $3, 3088($4)
    
    jr $31          # Retorna da função
# --- Funcao erase_red_ghost ---
# Esta funcao apaga o fantasma.
# Recebe: $4 (posicao atual do fantasma), $5 (base tela principal), $6 (base background)
erase_red_ghost:
    # Registradores temporários para cálculos
    # $8: Para guardar o offset da UG em relação ao inicio da tela principal.
    # $9: Para guardar o endereço completo da UG no background.
    # $10: Para guardar a cor carregada do background.

    # ------------------------------------------------------------------------------------------------------------------------------------------
    # APAGANDO CADA UG DO FANTASMA:
    # Para CADA 'sw $cor, OFFSET($4)' em 'desenharFantasmaVerde', há um bloco 'sub/add/lw/sw' correspondente aqui.
    # ------------------------------------------------------------------------------------------------------------------------------------------

    # Apagando UG em 0($4)
    sub $8, $4, $5       # Calcula o offset da UG em relação ao início da tela principal
    add $9, $6, $8       # Calcula o endereço da UG correspondente na cópia de fundo
    lw $10, 0($9)        # Carrega a cor de fundo para $10
    sw $10, 0($4)        # Salva a cor de fundo na tela principal (apaga a UG do fantasma)

    # Apagando UG em 4($4)
    addi $19, $4, 4      # Calcula o endereço da UG atual na tela principal
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 8($4)
    addi $19, $4, 8
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 508($4)
    addi $19, $4, 508
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 512($4)
    addi $19, $4, 512
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 516($4)
    addi $19, $4, 516
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 520($4)
    addi $19, $4, 520
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 524($4)
    addi $19, $4, 524
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1016($4)
    addi $19, $4, 1016
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1020($4)
    addi $19, $4, 1020
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1024($4)
    addi $19, $4, 1024
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1028($4)
    addi $19, $4, 1028
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1032($4)
    addi $19, $4, 1032
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1036($4)
    addi $19, $4, 1036
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1040($4)
    addi $19, $4, 1040
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1528($4)
    addi $19, $4, 1528
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1532($4)
    addi $19, $4, 1532
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1536($4)
    addi $19, $4, 1536
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1540($4)
    addi $19, $4, 1540
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1544($4)
    addi $19, $4, 1544
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 1548($4)
    addi $19, $4, 1548
    sub $8, $19, $5
    add $9, $6, $8
