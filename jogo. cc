desenhar_laranja:
    sw $7, 0($4)   # UG no ponto de referência
    sw $7, 4($4)   # UG 1 à direita
    sw $7, 8($4)   # UG 2 à direita

    sw $7, 512($4)  # UG 1 linha abaixo do ponto de referência
    sw $7, 508($4)  # UG 1 linha abaixo e 1 à esquerda (se 512 é uma linha completa)
    sw $7, 516($4)  # UG 1 linha abaixo e 1 à direita
    sw $7, 520($4)  # UG 1 linha abaixo e 2 à direita
    sw $7, 524($4)  # UG 1 linha abaixo e 3 à direita

    sw $7, 1016($4) # UG 2 linhas abaixo e X à esquerda/direita
    sw $3, 1020($4)
    sw $2, 1024($4)
    sw $3, 1032($4)
    sw $2, 1036($4)
    sw $7, 1028($4) # UG 2 linhas abaixo e Y à esquerda/direita
    sw $7, 1040($4) # UG 2 linhas abaixo e Z à esquerda/direita

    sw $7, 1528($4) # UG 3 linhas abaixo e X à esquerda/direita
    sw $3, 1532($4)
    sw $2, 1536($4)
    sw $7, 1540($4) # UG 3 linhas abaixo e Y à esquerda/direita
    sw $3, 1544($4)
    sw $2, 1548($4)
    sw $7, 1552($4) # UG 3 linhas abaixo e Z à esquerda/direita
    
    sw $7, 2040($4)
    sw $7, 2044($4)
    sw $7, 2048($4)
    sw $7, 2052($4)
    sw $7, 2056($4)
    sw $7, 2060($4)
    sw $7, 2064($4)
   
    sw $7, 2552($4)
    sw $7, 2556($4)
    sw $7, 2560($4)
    sw $7, 2564($4)
    sw $7, 2568($4)
    sw $7, 2572($4)
    sw $7, 2576($4)
    
    sw $7, 3064($4)
    sw $7, 3072($4)
    sw $7, 3080($4)
    sw $7, 3088($4)
    
    jr $31  
