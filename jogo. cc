    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 2040($4)
    addi $19, $4, 2040
    sub $8, $19, $5
    add $9, $6, $8
    lw $10, 0($9)
    sw $10, 0($19)

    # Apagando UG em 2044($4)
